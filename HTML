Prime.php

<!DOCTYPE html>

<html>

<head>

<meta charset="UTF-8">

<title>Prime Numbers Program in PHP</title>

</head>

<body>

<?php

$a=$_REQUEST["11"];

$c=0;

for($i=1;$i<=$a;$i++)

{

if($a%$i==0) $c++;

}

if($a==1)

echo "$a is neither prime nor composite number":

else if($c==2)

echo " Sa is a prime number";

else

echo"$a is a composite number";

?>

</body>


</html>

