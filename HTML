great.php:

<!DOCTYPE html>

<html>

<head>

<meta charset="UTF-8">

<title>Greatest of two Numbers</title>

</head>

<body>

<?php

$a=$_REQUEST["t1"];

$b=$_REQUEST["t2"];

if($a>$b)

echo "The greatest of two number is ".$a;

else

echo "The greatest of two number is ".$b;

?>

</body>


</html>

